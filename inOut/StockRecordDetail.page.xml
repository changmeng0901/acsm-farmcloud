<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
	 login-required="true">
	<begin-conversation join="true" flush-mode="MANUAL" />
	<param name="from" />
	<param name="goodsInfoId" value="#{stockControl.goodsInfoId}"/> 
	<param name="goodsInfoType" value="#{stockControl.goodsInfoType}"/> 
	<param name="goodsLevelId" value="#{stockControl.goodsLevelId}"/> 
	<param name="pageNum" value="#{stockControl.pageNum}"/> 
	<param name="batchCode" value="#{stockControl.batchCode}" />
	<param name="storageRoom" value="#{stockControl.storageRoom}" />
	<param name="operator" value="#{stockControl.operator}" />
	<param name="goodsLevel" value="#{stockControl.goodsLevel}" />
	<param name="goodsSource" value="#{stockControl.goodsSource}" />
	<param name="storageStart" value="#{stockControl.storageStart}" />
	<param name="storageEnd" value="#{stockControl.storageEnd}" />
</page>
