	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
	 login-required="true">
	<begin-conversation join="true" flush-mode="MANUAL" />
	<param name="from" />
	<param name="inventoryId" value="#{createGoodsBatchCheckControl.inventoryId}"/> 
	<param name="pageNum" value="#{createGoodsBatchCheckControl.pageNum}"/> 
	<param name="storageRoomId" value="#{inventoryRecord.storageRoomId}" />
	<param name="goodsType" value="#{inventoryRecord.goodsType}" />
	<param name="countStartTime" value="#{inventoryRecord.countStartTime}" />
	<param name="countEndTime" value="#{inventoryRecord.countEndTime}" />
	<param name="operaterId" value="#{inventoryRecord.operaterId}" />
	<param name="searchStr" value="#{inventoryRecord.searchStr}" />
</page>
