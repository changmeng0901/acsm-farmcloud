<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      login-required="true">
<begin-conversation join="true" flush-mode="MANUAL"/>
 <param name="positionsFrom"/>
 
  <navigation from-action="#{positionsHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/user/PositionsList.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{positionsHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/user/PositionsList.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{positionsHome.deletePos()}">
      <rule if-outcome="deletePosd">
         <end-conversation/>
         <redirect view-id="/user/PositionsList.xhtml"/>
      </rule>
   </navigation>
</page>
