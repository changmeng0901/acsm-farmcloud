<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd" login-required="true">

	<begin-conversation join="true" flush-mode="MANUAL" />
	<param name="recycleInfoFrom" />
	<param name="recycleInfoId" value="#{recycleInfoHome.recycleInfoRecycleInfoId}" />

	<navigation from-action="#{recycleInfoHome.remove}">
		<rule if-outcome="removed">
			<end-conversation />
			<redirect view-id="/system/RecycleInfoList.xhtml" />
		</rule>
	</navigation>
	
	<navigation from-action="#{recycleInfoHome.restore}">
		<rule if-outcome="restore">
			<end-conversation />
			<redirect view-id="/system/RecycleInfoList.xhtml" />
		</rule>
	</navigation>
</page>
