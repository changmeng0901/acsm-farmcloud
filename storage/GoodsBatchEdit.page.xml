<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/GoodsBatchList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{goodsBatchHome.wire}"/>

   <param name="goodsBatchFrom"/>
   <param name="goodsBatchGoodsBatchId" value="#{goodsBatchHome.goodsBatchGoodsBatchId}"/>


   <navigation from-action="#{goodsBatchHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/storage/GoodsBatchList.xhtml">
         </redirect>
      </rule>
   </navigation>

   <navigation from-action="#{goodsBatchHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/storage/GoodsBatchList.xhtml">
         </redirect>
      </rule>
   </navigation>

   <navigation from-action="#{goodsBatchHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/storage/GoodsBatchList.xhtml">
         </redirect>
      </rule>
   </navigation>

</page>
